    "@types/bcrypt": "^3.0.0",
    "@types/node": "^14.0.6",
    "@types/validator": "^13.0.0",
    "@types/yup": "^0.29.1",
    "builtin-modules": "^3.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.11.2",
